<h1><%= @title %></h1>

<h4><%= @year %></h4>
<h4>Category: <%= @cat %></h4>
<h4>Subcategory: <%= @subcat %></h4>

<table>

<thead>
<tr>
<th>Date</th>
<th>What</th>
<th>Amount</th>
</tr>
</thead>

<tbody>
<% @items.each do |item| %>
	<tr>
	<td><%= item.date %></td>
	<td><%= @whats[item.what_id] %></td>
	<td><%= item.amount %></td>
	</tr>
<% end %>
</tbody>

